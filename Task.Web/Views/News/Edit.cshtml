@model Task.DALModels.News
@{
    ViewBag.Title = "Edit";
}
@using (Html.BeginForm("Edit", "News"))
{
    <div class="text-center">
        @Html.ValidationSummary(true)
        <h1>
            Edit News</h1>
    </div>
    @Html.EditorForModel()
    <div class="text-center">
        <input type="submit" class="button" value="Edit" />
        <input type="button" class="button" value="Cancel" onclick="goToPage('@Url.Action("Index", "News")')" />
    </div>
}
